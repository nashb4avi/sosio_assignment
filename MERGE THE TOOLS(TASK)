def merge_the_tools(string, k):         
    list1=[]
    for i in range(0,len(string),k):
        list1.append(string[i:i+k])
    for i in list1:
        list2=[]
        for char in i:
            if char not in list2:
                list2.append(char)
        print(''.join(list2)) 
if __name__ == '__main__':
string, k = input(), int(input())
merge_the_tools(string, k)
        
